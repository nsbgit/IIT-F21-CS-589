<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project AccountClassTestDriver">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.workspace" value="${dir.buildfile}/EclipseWorkspace"/>
    <property name="dir.jarfile" value="${dir.workspace}/AccountClassTestDriver/executable"/>
    <target name="create_run_jar">
        <jar destfile="${dir.jarfile}/TestDriver1.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="TestDriver"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="${dir.workspace}/AccountClassTestDriver/bin"/>
        </jar>
    </target>
</project>
